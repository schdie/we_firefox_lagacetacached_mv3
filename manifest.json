{
  "description": "Cached content retrieval for lagaceta.com.ar",
  "manifest_version": 3,
  "name": "LG Cached",
  "version": "3.29",
  
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },

  "permissions": [
    "storage", "webRequest", "webRequestBlocking" 
  ],
  
  "host_permissions": [
	"*://webcache.googleusercontent.com/*", "*://*.lagaceta.com.ar/*", "*://*.lavoz.com.ar/*", "*://*.scuore.com.ar/*"
	],
  
	"web_accessible_resources": [
    {
      "resources": [ "controller.js", "content.css", "content.js", "popup.html", "popup.css", "popup.js" ],
      "matches": [ "*://*.lagaceta.com.ar/*" ]
    }
  ],

  "background": {
		"scripts": ["background.js"]
  },

  "page_action": {
	"show_matches": ["*://*.lagaceta.com.ar/*"],
    "default_popup": "popup.html",
		"default_icon": {
      "48": "/icons/48.png",
      "96": "/icons/96.png",
      "128": "/icons/128.png"
    }
  },
  
  "content_scripts": [
  {
    "matches": ["*://*.lagaceta.com.ar/*"],
    "css": ["content.css"],
    "js": ["content.js"],
    "run_at": "document_start"
    }
  ],

  "browser_specific_settings": {
    "gecko": {
			"id": "{400be594-34dd-4ae0-a0a4-20b5eca8473c}",
      "strict_min_version": "109.0"
    }
  }
  
}
